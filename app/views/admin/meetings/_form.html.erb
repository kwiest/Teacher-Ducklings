<%= f.input :date, as: :string, input_html: { size: 40, class: 'datepicker' } %>
<%= f.input :time, input_html: { size: 40 } %>
<%= f.association :moderator, collection: @admins, label_method: 'full_name', include_blank: false %>
<%= f.association :user, label: 'Meet with', collection: @users, label_method: 'full_name', include_blank: false %>
<%= f.association :video, label_method: 'verbose_title', include_blank: false %>
<%= f.input :confirmed, as: :boolean %>
